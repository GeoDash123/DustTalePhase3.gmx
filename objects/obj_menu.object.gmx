draw_text(320, 360, "Exit");

}

} else if pagina = 1 { //Ajustes

draw_sprite_ext(spr_caja_ajustes, 0, 320, 320, 2, 2, 0, c_white, 1);

draw_set_halign(fa_center);
draw_text(320, 455, "Press X to return to the menu");
draw_set_color(c_gray);
draw_text(320, 400, "UP/DOWN to move &amp; LEFT/RIGHT to adjust");
draw_set_color(c_white);
draw_set_halign(fa_left);
draw_set_valign(fa_center);
draw_text(40, 240, "Master Volume" + " [" + string(round(global.volumen * 100)) + "%]");
if global.bmusic = 1 { draw_text(350, 240, "Battle Music [ON]"); } else { draw_text(350, 240, "Battle Music [OFF]"); }

if global.ar = 1 { draw_text(40, 280, "Fast Reset [ON]"); } else { draw_text(40, 280, "Fast Reset [OFF]"); }

if global.nohit = 1 { draw_text(350, 280, "NO HIT [ON]"); } else { draw_text(350, 280, "NO HIT [OFF]"); }

draw_text(40, 320, "HP bar color (" + string(global.contador) + "/13)");

draw_set_color(global.bcolor);
draw_rectangle(40, 340, 180, 360, 0);
draw_set_color(c_white);

draw_text(350, 320, "Change name");
draw_text(350, 350, "[" + nombreM + " ]");


if opcion = 1 {

draw_set_color(c_yellow);
draw_text(40, 240, "Master Volume" + " [" + string(round(global.volumen * 100)) + "%]");

} else if opcion = 2 {

draw_set_color(c_yellow);
if global.ar = 1 { draw_text(40, 280, "Fast Reset [ON]"); } else { draw_text(40, 280, "Fast Reset [OFF]"); }

} else if opcion = 3 {

draw_set_color(c_yellow);
draw_text(40, 320, "HP bar color (" + string(global.contador) + "/13)");

} else if opcion = 4 {

draw_set_color(c_yellow);
if global.bmusic = 1 { draw_text(350, 240, "Battle Music [ON]"); } else { draw_text(350, 240, "Battle Music [OFF]"); }

} else if opcion = 5 {

draw_set_color(c_yellow);
if global.nohit = 1 { draw_text(350, 280, "NO HIT [ON]"); } else { draw_text(350, 280, "NO HIT [OFF]"); }

} else {

draw_set_color(c_yellow);
draw_text(350, 320, "Change name");
draw_text(350, 350, "[" + nombreM + " ]");

}

} else if pagina = 2 { //Elegir Dificultad

draw_sprite_ext(spr_caja_dificultad, 0, 320, 300, 2, 2, 0, c_white, 1);

draw_set_halign(fa_center);
draw_set_color(c_white);
draw_text(320, 360, "Choose difficulty");
draw_set_color(c_gray);
if global.hardmode = 0 { 

draw_sprite_ext(spr_caja_demo, 0, 320, 257, 1.5, 1.5, 0, c_white, 1);
draw_text(320, 390, "&lt;- NORMAL -&gt;"); } else { 

draw_sprite_ext(spr_caja_demo, 1, 320, 257, 1.5, 1.5, 0, c_white, 1);
draw_text(320, 390, "&lt;- HARDMODE -&gt;"); }

draw_set_color(c_white);
draw_text(320, 450, "Press X to return to the menu");

}
