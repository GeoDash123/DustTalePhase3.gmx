            <kind>1</kind>
            <string>///Dibujar Menu

if instance_exists(obj_rombo_caja) || room = room_nombre || instance_exists(obj_rombo_caja_nombre) { exit; }

//Logo
draw_sprite_ext(spr_logo_rombo, 0, 320, yPos2, 1, 1, angulo, c_white, 1);

draw_set_font(fnt_menu);
draw_set_color(c_white);

if pagina = 0 { //Menu Principal

draw_set_halign(fa_center);
draw_text(320, 240, "Start");
draw_text(320, 280, "Settings");
if opcion != 3 { draw_text(320, 320, "Credits"); }
draw_text(320, 360, "Exit");

if opcion = 1 {

