        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Controles

nombreM = string_upper(global.nombre);

if instance_exists(obj_rombo_caja) || room = room_nombre || instance_exists(obj_rombo_caja_nombre) { exit; }

/*Salir de Ajustes*/
if pagina = 1 &amp;&amp; KeyCont.key_secondary[0] = 1 &amp;&amp; locksalir = 0 {

instance_create(320, 240, obj_rombo_caja);
locksalir++;

}
/*Salir de Ajustes*/



if pagina = 2 &amp;&amp; KeyCont.key_primary[0] = 1 &amp;&amp; lock = 0 {

t = instance_create(0,0,obj_transicion_manipulable);
t.orden = room_preparar_combate;
audio_play_sound(snd_seleccion, 0, false);
lock++;
audio_stop_sound(snd_menu);

}


if pagina = 2 &amp;&amp; KeyCont.key_secondary[0] = 1 &amp;&amp; lock = 0 {

t = instance_create(0,0,obj_transicion_manipulable);
t.orden = room_menu;
lock++;

}



/*Ir a elegir nombre desde Ajustes*/
if pagina = 1 &amp;&amp; KeyCont.key_primary[0] = 1 &amp;&amp; locksalir = 0 &amp;&amp; opcion = 3 {

instance_create(320, 240, obj_rombo_caja_nombre);
locksalir++;

}
/*Ir a elegir nombre desde Ajustes*/


if KeyCont.key_down[0] = 1 &amp;&amp; lock = 0 {
opcion++; 
audio_play_sound(snd_move, 0, false);
}

if KeyCont.key_up[0] = 1 &amp;&amp; lock = 0 { 
opcion--; 
audio_play_sound(snd_move, 0, false);
}

if (pagina = 1 || pagina = 2) {
if KeyCont.key_right[0] = 1 || KeyCont.key_left[0] = 1 { 

if opcion != 6 {
audio_play_sound(snd_move, 0, false); } } }


if pagina = 0 {

if opcion &lt; 1 { opcion = 4; }
if opcion &gt; 4 { opcion = 1; }

} else if pagina = 1 {

if opcion &lt; 1 { opcion = 6; }
if opcion &gt; 6 { opcion = 1; }

}


if pagina = 0 { //Esta en el menu principal

if opcion = 1 &amp;&amp; KeyCont.key_primary[0] = 1 &amp;&amp; lock = 0 { //Elegir dificultad

audio_play_sound(snd_seleccion, 0, false);
t = instance_create(0,0,obj_cambiar_pagina);
t.siguiente = 2;
lock++;

}


if opcion = 2 &amp;&amp; KeyCont.key_primary[0] = 1 &amp;&amp; lock = 0 { //Ajustes

audio_play_sound(snd_seleccion, 0, false);
t = instance_create(0,0,obj_cambiar_pagina);
t.siguiente = 1;
lock++;

}


if opcion = 3 &amp;&amp; KeyCont.key_primary[0] = 1 &amp;&amp; lock = 0 { //Creditos

audio_play_sound(snd_seleccion, 0, false);
t = instance_create(0,0,obj_transicion_manipulable);
t.orden = room_creditos
lock++;

}


if opcion = 4 &amp;&amp; KeyCont.key_primary[0] = 1 &amp;&amp; lock = 0 { //Salir

audio_play_sound(snd_seleccion, 0, false);
instance_create(0,0,obj_transicion_manipulable);
with obj_transicion_manipulable { orden = room_salir; }
lock++;

}

} else if pagina = 1 { //Esta en los ajustes

if KeyCont.key_right[0] = 1 &amp;&amp; opcion = 1 { global.volumen+=0.05; }
if KeyCont.key_left[0] = 1 &amp;&amp; opcion = 1 { global.volumen-=0.05; }

if (KeyCont.key_right[0] = 1 || KeyCont.key_left[0] = 1) &amp;&amp; opcion = 2 {
if global.ar = 1 { global.ar = 0; } else { global.ar = 1; }
}



if opcion = 3 {

if KeyCont.key_right[0] = 1 { global.contador++; }
if KeyCont.key_left[0] = 1 { global.contador--; }

}

if (KeyCont.key_right[0] = 1 || KeyCont.key_left[0] = 1) &amp;&amp; opcion = 4 {
if global.bmusic = 1 { global.bmusic = 0; } else { global.bmusic = 1; }
}

if (KeyCont.key_right[0] = 1 || KeyCont.key_left[0] = 1) &amp;&amp; opcion = 5 {
if global.nohit = 1 { global.nohit = 0; } else { global.nohit = 1; }
}


switch(global.contador) {

    case 1: global.bcolor = c_aqua; break;
    case 2: global.bcolor = c_blue; break;
    case 3: global.bcolor = c_fuchsia; break;
    case 4: global.bcolor = c_gray; break;
    case 5: global.bcolor = c_green; break;
    case 6: global.bcolor = c_lime; break;
    case 7: global.bcolor = c_olive; break;
    case 8: global.bcolor = c_orange; break;
    case 9: global.bcolor = c_purple; break;
    case 10: global.bcolor = c_silver; break;
    case 11: global.bcolor = c_teal; break;
    case 12: global.bcolor = c_white; break;
    case 13: global.bcolor = c_yellow; break;

}



if global.volumen &lt;= 0 { global.volumen = 0; }
if global.volumen &gt;= 1.25 { global.volumen = 1.25; }

if global.contador &gt; 13 { global.contador = 1; }
if global.contador &lt; 1 { global.contador = 13; }


} else if pagina = 2 { //Esta en la seleccion de dificultad

if KeyCont.key_right[0] = 1 || KeyCont.key_left[0] = 1 { 
if global.hardmode = 0 { global.hardmode = 1; } else { global.hardmode = 0; }
}

}
